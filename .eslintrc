{
	"env": {
		"es6": true,
		"node": true
	},
	"rules": {
		"comma-dangle": [2, "never"],
		"no-cond-assign": [2, "always"],
		"no-constant-condition": 2,
		"no-debugger": 1,
		"no-dupe-args": 2,
		"no-dupe-keys": 2,
		"no-duplicate-case": 2,
		"no-empty-character-class": 2,
		"no-empty": 1,
		"no-ex-assign": 2,
		"no-extra-boolean-cast": 1,
		"no-extra-parens": [1, "all"],
		"no-extra-semi": 2,
		"no-func-assign": 2,
		"no-inner-declarations": 2,
		"no-invalid-regexp": 2,
		"no-irregular-whitespace": 2,
		"no-negated-in-lhs": 2,
		"no-obj-calls": 2,
		"no-regex-spaces": 1,
		"no-sparse-arrays": 2,
		"no-unexpected-multiline": 1,
		"no-unreachable": 2,
		"use-isnan": 2,
		"valid-typeof": 2,

		"eqeqeq": 2,
		"curly": 1,
		"complexity": [1, 100],
		"consistent-return": 1,
		"dot-location": [1, "property"],
		"dot-notation": 1,
		"guard-for-in": 1,
		"no-caller": 2,
		"no-case-declarations": 1,
		"no-else-return": 1,
		"no-empty-label": 2,
		"no-empty-pattern": 2,
		"no-eq-null": 2,
		"no-eval": 1,
		"no-extend-native": 2,
		"no-extra-bind": 1,
		"no-fallthrough": 1,
		"no-floating-decimal": 1,
		"no-implicit-coercion": 2,
		"no-implied-eval": 1,
		"no-invalid-this": 2,
		"no-iterator": 2,
		"no-lone-blocks": 1,
		"no-loop-func": 1,
		"no-multi-spaces": 1,
		"no-native-reassign": 2,
		"no-new-wrappers": 1,
		"no-new": 2,
		"no-proto": 2,
		"no-redeclare": 2,
		"no-return-assign": 1,
		"no-self-compare": 2,
		"no-throw-literal": 2,
		"no-unused-expressions": 2,
		"no-useless-call": 2,
		"no-useless-concat": 2,
		"no-void": 2,
		"no-with": 2,
		"radix": 1,
		"wrap-iife": [2, "inside"],
		"yoda": 2,

		"strict": [2, "global"],

		"no-shadow-restricted-names": 2,
		"no-undef-init": 2,
		"no-undef": 2,
		"no-unused-vars": 2,

		"callback-return": 2,
		"handle-callback-err": 2,
		"no-process-exit": 1,
		"no-path-concat": 2,
		"no-sync": 1,

		"array-bracket-spacing": [1, "never"],
		"block-spacing": [1, "always"],
		"brace-style": [1, "1tbs", { "allowSingleLine": true }],
		"camelcase": 1,
		"comma-spacing": [1, {
			"before": false,
			"after": true
		}],
		"comma-style": [1, "last"],
		"computed-property-spacing": [1, "never"],
		"consistent-this": [1, "self"],
		"func-names": 1,
		"func-style": [1, "declaration"],
		"id-length": [1, {
			"min": 1,
			"max": 60
		}],
		"indent": [2, "tab"],
		"key-spacing": [1, {
			"beforeColon": false,
			"afterColon": true
		}],
		"linebreak-style": [1, "unix"],
		"max-depth": [1, 6],
		"max-len": [1, 120],
		"max-nested-callbacks": [1, 3],
		"max-params": [1, 5],
		"max-statements": [1, 200],
		"new-cap": 1,
		"no-lonely-if": 1,
		"no-mixed-spaces-and-tabs": 1,
		"no-negated-condition": 1,
		"no-nested-ternary": 1,
		"no-spaced-func": 1,
		"no-trailing-spaces": 1,
		"no-underscore-dangle": 1,
		"no-unneeded-ternary": 1,
		"object-curly-spacing": [1, "never"],
		"operator-assignment": [1, "always"],
		"operator-linebreak": [1, "after"],
		"quote-props": [1, "as-needed"],
		"quotes": [2, "single"],

		"semi-spacing": 1,
		"semi": [1, "always"],
		"space-after-keywords": [1, "always"],
		"space-before-blocks": [1, "always"],
		"space-before-keywords": [1, "always"],
		"space-in-parens": [1, "never"],
		"space-infix-ops": 1,
		"space-return-throw-case": 1,
		"space-unary-ops": 1,

		"arrow-body-style": [1, "as-needed"],
		"arrow-parens": [1, "as-needed"],
		"arrow-spacing": 1,
		"constructor-super": 2,
		"generator-star-spacing": 1,
		"no-arrow-condition": 2,
		"no-class-assign": 2,
		"no-const-assign": 2,
		"no-dupe-class-members": 2,
		"no-this-before-super": 2,
		"no-var": 1,
		"object-shorthand": 1,
		"prefer-arrow-callback": 2,
		"prefer-const": 1,
		"prefer-spread": 1,
		"require-yield": 1
	}
}
